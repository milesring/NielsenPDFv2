<Window x:Class="NielsenPDFv2.Views.PDFPreview"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NielsenPDFv2.Views"
        xmlns:vm="clr-namespace:NielsenPDFv2.ViewModels"
        mc:Ignorable="d"
        Title="PDFPreview" Height="450" Width="450"
        ResizeMode="NoResize"
        WindowStyle="None"
        MouseLeave="Window_MouseLeave">
    <Window.DataContext>
        <vm:PDFPreviewViewModel/>
    </Window.DataContext>
    <Grid>
        <WebBrowser Margin="10,10,10,10" x:Name="webBrowser" VerticalAlignment="Stretch"/>
    </Grid>
</Window>
